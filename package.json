{
  "name": "gatsby-starter-hello-world",
  "description": "Gatsby hello world starter",
  "license": "MIT",
  "scripts": {
    "develop": "GATSBY_BASE_URL='http://localhost:8000' gatsby develop -H localhost",
    "build": "GATSBY_BASE_URL='https://mattlane.us' gatsby build",
    "serve": "gatsby serve",
    "deploy": "GATSBY_BASE_URL='https://mattlane.us' gatsby build && s3-deploy './public/**' --cwd './public/' --region us-east-1 --bucket mattlane.us --deleteRemoved && aws-cloudfront-invalidate $npm_config_distribution_id",
    "test": "jest"
  },
  "dependencies": {
    "animate.css": "^3.6.1",
    "aws-cloudfront-invalidate": "^0.1.3",
    "d3": "^4.13.0",
    "d3-fetch": "^1.1.0",
    "d3-force-bounce": "^0.5.5",
    "d3-force-surface": "^0.5.5",
    "d3-transition": "^1.1.1",
    "font-awesome": "^4.7.0",
    "gatsby": "^1.9.255",
    "gatsby-link": "^1.6.41",
    "gatsby-plugin-feed": "^1.3.21",
    "gatsby-plugin-google-analytics": "^1.0.31",
    "gatsby-plugin-react-helmet": "^2.0.11",
    "gatsby-plugin-resolve-src": "^1.1.3",
    "gatsby-plugin-sitemap": "^1.2.23",
    "gatsby-plugin-typography": "^1.7.18",
    "gatsby-source-filesystem": "^1.5.34",
    "gatsby-transformer-remark": "^1.7.40",
    "katex": "^0.9.0",
    "marksy": "^6.0.3",
    "odex": "^2.0.4",
    "polished": "^1.9.2",
    "prop-types": "^15.6.1",
    "rc-slider": "^8.6.1",
    "react-animate-on-scroll": "^1.6.2",
    "react-helmet": "^5.2.0",
    "react-move": "^2.7.0",
    "react-select": "^1.2.1",
    "react-twitter-widgets": "^1.7.1",
    "styled-components": "^2.4.0",
    "topojson": "^3.0.2",
    "typography-theme-alton": "^0.15.10"
  },
  "devDependencies": {
    "babel-jest": "^22.4.3",
    "jest": "^22.4.3",
    "s3-deploy": "^0.8.0"
  }
}
