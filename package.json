{
  "name": "mattlane.us",
  "description": "Matt Lane personal Gatsby site",
  "repository": {
    "type": "git",
    "url": "git://github.com/mmmaaatttttt/personal-site.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "GATSBY_BASE_URL='https://mattlane.us' gatsby build",
    "deploy": "GATSBY_BASE_URL='https://mattlane.us' gatsby build && s3-deploy './public/**' --cwd './public/' --region us-east-1 --bucket mattlane.us --deleteRemoved && aws-cloudfront-invalidate $npm_config_distribution_id",
    "develop": "npm run indices && GATSBY_BASE_URL='http://localhost:8000' gatsby develop -H localhost",
    "indices": "create-index -i ./src/story_components/{atoms,molecules,organisms,templates,} ./src/providers ./src/hooks ./src/contexts",
    "serve": "gatsby serve",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:clear": "npx jest --clearCache"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.5.8",
    "@mdx-js/react": "^1.5.8",
    "acorn": "^6.4.1",
    "ajv": "^6.12.1",
    "animate.css": "^3.7.2",
    "aws-cloudfront-invalidate": "^0.1.3",
    "d3": "^5.15.1",
    "d3-force-bounce": "^0.5.7",
    "d3-force-surface": "^0.5.7",
    "d3-transition": "^1.3.2",
    "gatsby": "^2.20.25",
    "gatsby-image": "^2.3.4",
    "gatsby-link": "^2.3.4",
    "gatsby-plugin-feed-mdx": "^1.0.1",
    "gatsby-plugin-google-analytics": "^2.2.4",
    "gatsby-plugin-mdx": "^1.1.9",
    "gatsby-plugin-react-helmet": "^3.2.4",
    "gatsby-plugin-resolve-src": "^2.0.0",
    "gatsby-plugin-sharp": "^2.5.6",
    "gatsby-plugin-sitemap": "^2.3.4",
    "gatsby-plugin-styled-components": "^3.2.3",
    "gatsby-plugin-typography": "^2.4.3",
    "gatsby-remark-images": "^3.2.4",
    "gatsby-source-filesystem": "^2.2.4",
    "gatsby-transformer-csv": "^2.2.3",
    "gatsby-transformer-sharp": "^2.4.6",
    "graphql": "^14.6.0",
    "jstat": "^1.9.2",
    "katex": "^0.10.2",
    "lodash": "^4.17.15",
    "odex": "^2.0.4",
    "polished": "^2.3.3",
    "prop-types": "^15.7.2",
    "rc-slider": "^8.7.1",
    "react": "^16.13.1",
    "react-animate-on-scroll": "^2.1.5",
    "react-dom": "^16.13.1",
    "react-draggable": "^4.3.1",
    "react-helmet": "^5.2.1",
    "react-mailchimp-subscribe": "^2.1.0",
    "react-measure": "^2.3.0",
    "react-move": "^2.9.1",
    "react-redux": "^6.0.1",
    "react-select": "^2.4.4",
    "react-twitter-widgets": "^1.7.1",
    "react-typography": "^0.16.19",
    "redux": "^4.0.5",
    "styled-components": "^4.4.1",
    "topojson": "^3.0.2",
    "typography": "^0.16.19",
    "typography-theme-alton": "^0.16.19"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-preset-gatsby": "^0.1.11",
    "create-index": "^2.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.3.0",
    "react-test-renderer": "^16.13.1",
    "s3-deploy": "^1.4.0"
  }
}
